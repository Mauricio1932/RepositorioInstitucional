const { app, BrowserWindow } = require('electron');

app.on('ready', () => {
    let win = new BrowserWindow({
        webPreferences: {
            nodeIntegration: true,
            contextIsolation: false,
        }
    });

    win.loadFile("index.html"); //cargado directo

    win.show()

    win.on('closed', () => {
            win = null;
            app.quit();
        }

    )

})